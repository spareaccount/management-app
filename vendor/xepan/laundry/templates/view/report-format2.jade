script.
    $("#btnPrint").on("click", function () {
        var divContents = $("#dvContainer").html();
        var printWindow = window.open('', '', 'height=400,width=800');
        printWindow.document.write('<html><head><title>DIV Contents</title>');
        printWindow.document.write('</head><body >');
        printWindow.document.write(divContents);
        printWindow.document.write('</body></html>');
        printWindow.document.close();
        printWindow.print();
    });
.xepan-grid.main-box.clearfix.atk-table.atk-table-zebra.atk-table-bordered(id="{$_name}" class="{$class}")
    div(id="btnPrint" style="cursor: pointer; cursor: hand;")
        &nbsp;&nbsp;
        i 
            .fa.fa-print
        &nbsp;&nbsp;
        span print
    {Pannel}
    header.main-box-header.clearfix.atk-actions(class=" {$class} ")
        div.filter-block( style="100%")
            .pull-right {$grid_buttons} 
            .form-group.pull-right.xepan-filter-form
                span(class="form-control pull-left")|{$quick_search}
    {/Pannel}
    div#dvContainer
        .main-box-body.clearfix.table
            table.table.user-list.table-hover(style="width:100%" border="1")
                {header}
                thead
                    {cols}{col}
                    th
                        span S.No.
                    th
                        span Date
                    th
                        span Number of set
                    th
                        span Number of Quilt
                    {/}{/}
                {/}
                {$subheader}

                tbody
                    {rows}{row}
                    tr(class="{$odd_even}" data-id="{$id}")
                        {cols}
                        td
                          span {$sno}
                        td
                          span {$created_date}
                        td
                          span {$set_amount}
                        td
                          span {$quilt_amount}
                        {/}
                    {/}{/}
                    {not_found}
                    div.row
                        .col-md-12.text-center.alert.alert-warning(role="alert")
                            span.icon-attention.fa.fa-exclamation-triangle  &nbsp;{$not_found_message}
                    {/}

                {totals}
                tfoot
                    tr(class="totals")
                        {cols}{col}
                        <td {$tdparam}>
                        {$content}
                        </td>
                        {/}{/}
                {/}
                table
                    td
                        th(style="width:18%;") Total
                        th(style="width:18%;")
                        th(style="width:30%;")
                            span(style="float:left;") {$set_total}
                        th(style="width:30%;")
                            span(style="float:left;") {$quilt_total}
                        th(style="width:18%;")

        {$Paginator}